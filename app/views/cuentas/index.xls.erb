<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">ID</Data></Cell>
        <Cell><Data ss:Type="String">Proyecto</Data></Cell>
        <Cell><Data ss:Type="String">Saldo</Data></Cell>
      </Row>
    <% @cuentas.each do |cuenta| %>
      <Row>
        <Cell><Data ss:Type="String"><%= cuenta.id %></Data></Cell>
        <Cell><Data ss:Type="String"><%= cuenta.proyecto.codigo %></Data></Cell>
        <Cell><Data ss:Type="Number"><%= cuenta.saldo %></Data></Cell>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>