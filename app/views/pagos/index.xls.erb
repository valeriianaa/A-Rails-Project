<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">ID</Data></Cell>
        <Cell><Data ss:Type="String">Fecha</Data></Cell>
        <Cell><Data ss:Type="String">Proyecto CÃ³digo</Data></Cell>
        <Cell><Data ss:Type="String">Responsable del contrato</Data></Cell>
        <Cell><Data ss:Type="String">Numero de Identificacion</Data></Cell>
        <Cell><Data ss:Type="String">Fecha de Nacimiento</Data></Cell>
        <Cell><Data ss:Type="String">Pais</Data></Cell>
        <Cell><Data ss:Type="String">Provincia</Data></Cell>
        <Cell><Data ss:Type="String">Ciudad</Data></Cell>
        <Cell><Data ss:Type="String">Calle</Data></Cell>
        <Cell><Data ss:Type="String">Numero</Data></Cell>
        <Cell><Data ss:Type="String">Piso</Data></Cell>
        <Cell><Data ss:Type="String">Departamento</Data></Cell>
        <Cell><Data ss:Type="String">Telefono</Data></Cell>
        <Cell><Data ss:Type="String">Correo Electronico</Data></Cell>
      </Row>
    <% @pagos.each do |pago| %>
      <Row>
        <Cell><Data ss:Type="String"><%= pago.id.to_s %></Data></Cell>
        <Cell><Data ss:Type="String"><%= pago.nombre %></Data></Cell>
        <Cell><Data ss:Type="String"><%= pago.apellido %></Data></Cell>
        <Cell><Data ss:Type="String"><%= pago.tipo_documento.nombre %></Data></Cell>
        <Cell><Data ss:Type="String"><%= pago.nroIdentificacion %></Data></Cell>
        <Cell><Data ss:Type="Data"><%= pago.fechaNacimiento %></Data></Cell>
        <Cell><Data ss:Type="String"><%= pago.pais.nombre %></Data></Cell>
        <Cell><Data ss:Type="String"><%= pago.provincia.nombre %></Data></Cell>
        <Cell><Data ss:Type="String"><%= pago.ciudad.nombre %></Data></Cell>
        <Cell><Data ss:Type="String"><%= pago.calle %></Data></Cell>
        <Cell><Data ss:Type="String"><%= pago.nroDomicilio %></Data></Cell>
        <Cell><Data ss:Type="String"><%= pago.piso %></Data></Cell>
        <Cell><Data ss:Type="String"><%= pago.dpto %></Data></Cell>
        <Cell><Data ss:Type="String"><%= pago.telefono %></Data></Cell>
        <Cell><Data ss:Type="String"><%= pago.email %></Data></Cell>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>